def count_distinct_pairs(t,K):
    seen = set()
    distinct_pairs = set()
    for num in t:
        complement = K - num
        if complement in seen:
            pair = (min(num, complement), max(num, complement))
            distinct_pairs.add(pair)
        seen.add(num)
    return len(distinct_pairs)
t = tuple(map(int, input().split(',')))
K = int(input())
result = count_distinct_pairs(t, K)
print(result)

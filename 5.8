def extract_common_chars(s1, s2, n):
    common_chars = []
    for char in s1:
        if char in s2 and char not in common_chars:
            common_chars.append(char)
            if len(common_chars) == n:
                break
    return ''.join(common_chars)
s1 = input().strip()
s2 = input().strip()
n = int(input().strip())
print(extract_common_chars(s1, s2, n))

# Function to check if a character is an alphabet
def is_alphabet(char):
    return char.isalpha()

# Function to reverse the string without affecting special characters
def reverse_string_with_special_chars(S):
    # Convert string to list for easier manipulation
    S = list(S)
    # Initialize pointers
    left = 0
    right = len(S) - 1
    
    # Move pointers towards each other
    while left < right:
        # If left pointer is not alphabet, move it to the right
        if not is_alphabet(S[left]):
            left += 1
        # If right pointer is not alphabet, move it to the left
        elif not is_alphabet(S[right]):
            right -= 1
        # If both pointers are alphabet, swap characters
        else:
            S[left], S[right] = S[right], S[left]
            left += 1
            right -= 1
    
    # Convert list back to string and return
    return ''.join(S)
  # Input string
S = input().strip()

# Reverse the string without affecting special characters and print the result
print(reverse_string_with_special_chars(S))  

def count_frequency(arr):
    frequency = {}
    for num in arr:
        if num in frequency:
            frequency[num] += 1
        else:
            frequency[num] = 1
   
    for num, freq in frequency.items():
        print(f"{num} occurs {freq} times")

# Input
n = int(input())
arr = []
for _ in range(n):
    arr.append(int(input()))

# Output
count_frequency(arr)

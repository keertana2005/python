def main():
    import sys
    input = sys.stdin.read
    data = input().split()
    
    X = int(data[0])
    shoe_sizes = list(map(int, data[1:X+1]))
    N = int(data[X+1])
    
    from collections import Counter
    inventory = Counter(shoe_sizes)
    
    index = X + 2
    total_revenue = 0
    
    for _ in range(N):
        size = int(data[index])
        price = int(data[index + 1])
        index += 2
        
        if inventory[size] > 0:
            total_revenue += price
            inventory[size] -= 1
    
    print(total_revenue)

if __name__ == "__main__":
    main()

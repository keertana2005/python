def productDigits(n):
    digits = [int(d) for d in str(n)] 
    product_even = 1
    sum_odd = 0
    for i, digit in enumerate(digits):
        if (i + 1) % 2 == 0:  
            product_even *= digit  
        else:
            sum_odd += digit  
    if sum_odd == 0:
        return "False"
    if product_even % sum_odd == 0:
        return "True"
    else:
        return "False"

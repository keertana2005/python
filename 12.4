
def count_unique_pairs_with_difference(n, nums, k):
    count = 0
    for i in range(n):
        for j in range(i + 1, n):
            if abs(nums[i] - nums[j]) == k:
                count += 1
    return count
n = int(input())
nums = list(map(int, input().split()))
k = int(input())
result = count_unique_pairs_with_difference(n, nums, k)
print(result)
